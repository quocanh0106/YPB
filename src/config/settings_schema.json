  {
    "name": "Favicon",
    "settings": [
      {
        "type": "image_picker",
        "id": "favicon",
        "label": "Favicon image",
        "info": "Will be scaled down to 32 x 32px"
      }
    ]
  },
  {
    "name": "General settings",
    "settings": [
      {
        "type": "header",
        "content": "Logo"
      },
      {
        "type": "image_picker",
        "id": "logo",
        "label": "Logo image"
      },
      {
        "type": "range",
        "id": "logo_width",
        "min": 50,
        "max": 150,
        "step": 1,
        "default": 134,
        "unit": "px",
        "label": "Custom logo width"
      },
      {
        "type": "image_picker",
        "id": "logo_mb",
        "label": "Logo image"
      },
      {
        "type": "range",
